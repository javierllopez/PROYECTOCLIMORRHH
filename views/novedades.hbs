                            {{#ifmay totalesPendientes.totalGuardiasDiurnas 0}} <td>
                                {{formatoNumero totalesPendientes.totalGuardiasDiurnas}}</td>{{else}} <td>0</td>
                            {{/ifmay}}
                            {{/ifeq}}
