<div class="m-2 container border border-primary rounded-4 bg-primary text-white"></div>
<h1 class="text-center">{{Modulo}}</h1>
</div>
<div class="container-fluid m-2">
    <div class="row">
        <div class="col-6">
            <div class="card">
                <div class="card-header">
                    <h2 class="text-center">{{Titulo}}</h2>
                </div>
                <div class="card-body">
                    <h5 class="card-title">{{Texto}}</h5>

                </div>
                <div class="card-footer text-center">
                    {{#each Botones}}
                    <a href="{{this.link}}" class="btn btn-primary">{{this.texto}}</a>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>